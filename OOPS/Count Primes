Given an integer n, return the number of prime numbers that are strictly less than n.
class Solution:
    def countPrimes(self, n: int) -> int:
        def is_prime(num):
            if num <= 1:
                return False
            for i in range(2, int(num**0.5) + 1):
                if num % i == 0:
                    return False
            return True
        count = 0
        for num in range(2, n):
            if is_prime(num):
                count += 1
        return count
